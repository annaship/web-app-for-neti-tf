
<HYBRID>	::= <NAMED_HYBRID> | <HYBRID_FORMULA>

# Agrostis stolonifera L. × Polypogon monspeliensis (L.) Desf.
<HYBRID_FORMULA>	::= <SCIENTIFICNAME> <SPACER> <HYBRID_MARKER> <SPACER> (<SCIENTIFICNAME> | <EPITHETON> (<SPACER> <AUTHORSHIP>)? )

# ×Heucherella tiarelloides
<NAMED_HYBRID>	::= <HYBRID_MARKER> <SCIENTIFIC_NAME> | <MULTIPLICATION_SYMBOL> <EPITHETON> (<SPACER> <AUTHORSHIP>)? )


<AUTHORSHIP>	::= (<AUTHORTEAM_BRACKETS> <SPACER>)?   <AUTHORTEAM>  (<SPACER> "ex" "."? <SPACER> (<AUTHORTEAM_BRACKETS> <SPACER>)? <AUTHORTEAM> )?    
<AUTHORTEAM_BRACKETS>	::= "(" <SPACER> <AUTHORTEAM> <SPACER> ")"
<AUTHORTEAM>	::= (<AUTHOR> ((<SPACER> ("&"|"et"|","))? <SPACER> <AUTHOR>)*) 
<AUTHOR>	::= [A-Z] (<AUTHORCHAR>+ | ".") 

<SCIENTIFIC_NAME>	::= <MONOMIAL> | <SPECIESNAME> | <INFRASPECIESNAME>  
<INFRASPECIESNAME>	::= <SPECIESNAME> (<SPACER> <RANK>)? (<SPACER> <EPITHETON> (<SPACER> <AUTHORSHIP>)?)?
<SPECIESNAME>	::= (([A-Z] ".")|<MONOMIAL>) (<SPACE_HARD> <SUBGENUS>)? <SPACE_HARD> <EPITHETON> (<SPACER> <AUTHORSHIP>)? 

<SUBGENUS>	::= <LEFT_BRACKET> <UNINOMIAL> <RIGHT_BRACKET>

<SENSU>	::= ("sec" | "sensu") "\."?
<RANK>	::= 
<GENUS_RANK>	::= ("subtrib" | "subsect" | "subgen" | "trib") "\."?

<UNINOMIAL>	::= (([A-Z] | <DIGRAPH_UPPER>) <NAME_LETTERS>{2,}) | <TWO_LETTER_GENERA>
<TWO_LETTER_GENERA>	::= "Ca" | "Ea" | "Ge" | "Ia" | "Io" | "Ix" | "Lo" | "Oa" | "Ra" | "Ty" | "Ua" | "Aa" | "Ja" | "Zu" | "La" | "Qu" | "As" | "Ba"

# Abies sp. 2; Pinus spec.; 
<INFORMAL>	::= ("sp"|"spec"|"indet") "\."? (<SPACE>|<EPITHETON>)+
# uni-folia or 1-folia
<EPITHETON>	::= ([1-9]-)? <NAME_LETTERS>+

# really all latin characters with diacritics
<AUTHOR_LETTERS>	::= <NAME_LETTERS> | [âãåěçš]
<NAME_LETTERS>	::= [a-zïëöüäéèčáà-] | <DIGRAPH_LOWER>
<DIGRAPH_UPPER>	::= [ÆŒ]
<DIGRAPH_LOWER>	::= [æœ]
<NAME_QUALIFIER>	::= ("aff" | "cf" | "?" | "near" | "group" | "versus" | "vs") "."?  
<HYBRID_RANK>	::= "notho" <RANK>
<HYBRID_MARKER>	::= [×xX]
<MULTIPLICATION_SYMBOL>	::= "×"
<RANK>	::= ("subsp" | "ssp" | "var" | "forma" | "f") "."?  
<YEAR_WITH_CHARACTER>	::= <YEAR> [a-zA-Z]
<YEAR>	::= [12][0-9][0-9][0-9\?](\?)?
<LEFT_BRACKET>	::= "("
<RIGHT_BRACKET>	::= ")"
<SPACE>	::= \s*
<SPACE_HARD>	::= \s+








